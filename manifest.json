{
	"manifest_version": 2,
	"name": "GDPR compliance plugin",
	"version": "0.3",
	"description": "A description",
	"homepage_url": "https://gitlab.com/Dennis_Kalo/cookiemonitor",
	"browser_action": {
		"browser_style": true,
		"default_title": "Cookies",
		"default_popup": "dist/cookie_list.html",
		"default_icon": {
			"16": "icons/arch-linux.png",
			"32": "icons/arch-linux.png",
			"48": "icons/arch-linux.png"
		}
	},
	"permissions": [
		"cookies",
		"<all_urls>",
		"tabs"
	]
}