{
	"manifest_version": 2,
	"name": "GDPR compliance plugin",
	"version": "0.3",
	"description": "A description",
	"homepage_url": "https://gitlab.com/Dennis_Kalo/cookiemonitor",
  "background": {
    "scripts": ["src/background-script.js"]
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["src/content-script.js"]
  }],
	"permissions": [
		"cookies",
		"<all_urls>",
		"tabs"
	]
}
