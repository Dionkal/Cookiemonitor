{
	"manifest_version": 2.0,
	"name": "GDPR compliance plugin",
	"version": "0.1",
	"description": "A description",
	"homepage_url": "https://gitlab.com/Dennis_Kalo/cookiemonitor",
	"browser_action": {
		"browser_style": true,
		"default_title": "Cookies",
		"default_popup": "cookie_list.html",
		"default_icon": "icons/border-48.png"
	},
	"permissions": [
		"cookies",
		"<all_urls>",
		"tabs"
	]
}
